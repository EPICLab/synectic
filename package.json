{
  "name": "synectic",
  "version": "0.7.3",
  "full_name": "Synectic IDE",
  "description": "Research IDE for examining context-centric, problem-solving in development tools.",
  "main": "dist/main.js",
  "build": {
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "buildResources": "src/asset",
      "output": "release/"
    }
  },
  "scripts": {
    "prebuild": "npm run compile",
    "build": "electron-builder --dir",
    "prepack": "npm run compile",
    "pack": "electron-builder",
    "pack:mac": "electron-builder --arch=x64 --platform=darwin",
    "pack:win": "electron-builder --arch=x64 --platform=win32",
    "pack:linux": "electron-builder --arch=x64 --platform=linux",
    "check": "npm-check --no-emoji -s -u",
    "clean": "rimraf dist/* release/*",
    "compile": "webpack --config webpack.config.js --progress --colors",
    "prestart": "npm run compile",
    "start": "electron ./dist/main.js",
    "prelint": "npm run clean",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "pretest": "webpack --config webpack.test.config.js --progress --colors",
    "test": "mocha -u bdd -r ts-node/register -r ignore-styles -r jsdom-global/register dist/**/*.spec.js",
    "posttest": "rimraf 'dist/**/*.spec.{js,js.map}'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nelsonni/synectic.git"
  },
  "keywords": [
    "integrated-development-environment",
    "research-project",
    "cognitive",
    "cards"
  ],
  "author": "Nicholas Nelson <nelsonni@oregonstate.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nelsonni/synectic/issues"
  },
  "homepage": "https://github.com/nelsonni/synectic#readme",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "brace": "^0.11.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.9",
    "electron": "^1.8.2",
    "electron-builder": "^20.0.4",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.1",
    "npm-check": "^5.5.2",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.20.1",
    "ts-loader": "^3.5.0",
    "ts-mockito": "^2.2.9",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "tslint-no-unused-expression-chai": "0.0.3",
    "typescript": "^2.7.1",
    "webpack": "^3.11.0",
    "webpack-node-externals": "^1.6.0",
    "wildcards-entry-webpack-plugin": "^2.1.0"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.0",
    "@types/jquery": "^3.3.0",
    "@types/jqueryui": "^1.12.2",
    "@types/luxon": "^0.2.6",
    "@types/uuid": "^3.4.3",
    "fs-extra": "^5.0.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "luxon": "^0.5.3",
    "uuid": "^3.2.1"
  }
}
