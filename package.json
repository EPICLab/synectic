{
  "name": "synectic",
  "version": "0.0.1",
  "full_name": "Synectic IDE",
  "description": "Research IDE for examining context-centric, problem-solving in development tools.",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf ./dist/*",
    "prestart": "webpack --config webpack.config.js --progress --colors",
    "start": "electron ./dist/main.js",
    "lint": "npm run clean && tslint -c tslint.json -p tsconfig.json -t stylish",
    "test": "mocha -u bdd -r ts-node/register -r ignore-styles -r jsdom-global/register test/**/*.spec.ts",
    "checkDeps": "npm-check --skip-unused -u"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nelsonni/synectic.git"
  },
  "keywords": [
    "integrated-development-environment",
    "research-project",
    "cognitive",
    "cards"
  ],
  "author": "Nicholas Nelson <nelsonni@oregonstate.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nelsonni/synectic/issues"
  },
  "homepage": "https://github.com/nelsonni/synectic#readme",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.5.2",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "electron": "^1.7.10",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "npm-check": "^5.5.2",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "ts-mockito": "^2.2.7",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-loader": "^3.5.3",
    "tslint-no-unused-expression-chai": "0.0.3",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "@types/jquery": "^3.3.0",
    "@types/jqueryui": "^1.12.1",
    "@types/uuid": "^3.4.3",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "uuid": "^3.2.1"
  }
}
