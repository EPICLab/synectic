{
  "name": "synectic",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "build": "tsc && copyfiles ./src/**/*.html ./src/**/*.css ./dist/",
    "clean": "rimraf ./dist/*",
    "start": "npm run build && electron ./dist/src/main.js",
    "exec": "npm run build && node ./dist/src/main.js",
    "live": "tsc -w",
    "check": "npm-check --skip-unused -u",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "test": "mocha -u bdd -r ts-node/register -r ignore-styles -r jsdom-global/register test/**/*.spec.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.1.1",
    "@types/mocha": "^2.2.46",
    "chai": "^4.1.2",
    "copyfiles": "^1.2.0",
    "electron": "^1.7.10",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.0",
    "npm-check": "^5.5.2",
    "rimraf": "^2.6.2",
    "ts-mockito": "^2.2.7",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-no-unused-expression-chai": "0.0.3",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/uuid": "^3.4.3",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "uuid": "^3.2.1"
  }
}
