{
  "name": "synectic",
  "productName": "Synectic IDE",
  "description": "",
  "version": "0.6.0",
  "repository": "https://github.com/nelsonni/synectic",
  "author": "Nicholas Nelson <nelsonni@oregonstate.edu",
  "copyright": "Â© 2017, Nicholas Nelson",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Synectic --asar --overwrite --icon=lib/icon.icns --out=release-builds --ignore=test",
    "build-all": "npm run build-mac && npm run build-win && npm run build-linux",
    "build-mac": "electron-packager . Synectic --platform=darwin --arch=x64 --asar --overwrite --icon=lib/icon.icns --out=release-builds --ignore=test",
    "build-win": "electron-packager . Synectic --platform=win32 --arch=x64 --asar --overwrite --icon=lib/icon.icns --out=release-builds --ignore=test",
    "build-linux": "electron-packager . Synectic --platform=linux --arch=x64 --asar --overwrite --icon=lib/icon.icns --out=release-builds --ignore=test",
    "build-dev": "electron-packager . Synectic-dev --overwrite --no-prune --icon=lib/icon.icns --out=release-builds",
    "clean": "rm -Rf release-builds/*",
    "rebuild": "electron-rebuild -f",
    "test": "mocha --compilers js:babel-core/register --require jsdom-global/register ./test/*.js"
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "electron": "^1.8.1",
    "electron-mocha": "^4.0.3",
    "electron-packager": "^8.7.2",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^1.6.0",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.5.3",
    "spectron": "^3.7.2"
  },
  "dependencies": {
    "ace-code-editor": "^1.2.3",
    "jquery": "^3.2.1",
    "jquery-ui-bundle": "^1.12.1",
    "raphael": "^2.2.7",
    "slick-carousel": "^1.8.1",
    "username": "^3.0.0",
    "uuid": "^3.1.0"
  }
}
