{
  "name": "synectic",
  "version": "0.8.0",
  "full_name": "Synectic IDE",
  "description": "Research IDE for examining context-centric, problem-solving in development tools.",
  "main": "main.js",
  "scripts": {
    "check": "npm-check --no-emoji -s -u",
    "clean": "rimraf dist/* release/*",
    "compile": "webpack --mode production",
    "prestart": "npm run compile",
    "start": "electron ./dist/main.js",
    "prelint": "npm run clean",
    "lint": "tslint --force -c tslint.json -p tsconfig.json -t stylish",
    "pretest": "webpack --mode development",
    "test": "mocha -u bdd -r ts-node/register test/*.spec.ts",
    "xtest": "mocha -u bdd -r ts-node/register -r ignore-styles -r jsdom-global/register dist/**/*.spec.js",
    "posttest": "rimraf 'dist/**/*.spec.{js,js.map}'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nelsonni/synectic.git"
  },
  "keywords": [
    "integrated-development-environment",
    "research-project",
    "cognitive",
    "cards"
  ],
  "author": "Nicholas Nelson <nelsonni@oregonstate.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nelsonni/synectic/issues"
  },
  "homepage": "https://github.com/nelsonni/synectic#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/jsdom": "^11.0.6",
    "@types/luxon": "^1.2.1",
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.2",
    "@types/uuid": "^3.4.3",
    "ajv": "^6.5.2",
    "ajv-keywords": "^3.2.0",
    "brace": "^0.11.1",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "electron": "^2.0.4",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "luxon": "^1.3.1",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "npm-check": "^5.7.1",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.1.0",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^2.9.2",
    "uuid": "^3.3.2",
    "webpack": "^4.15.1",
    "webpack-command": "^0.4.1",
    "wildcards-entry-webpack-plugin": "^2.1.0"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/jquery": "^3.3.4",
    "@types/jqueryui": "^1.12.2",
    "fs-extra": "^6.0.1",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1"
  }
}
